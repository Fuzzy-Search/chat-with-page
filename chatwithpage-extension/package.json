{
  "name": "fuzzy-search",
  "displayName": "Chat with page. Free & open source",
  "version": "0.0.7",
  "description": "Perform a fuzzy search on the current page.",
  "author": "Robert Lukoshko",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
    "dev:next": "next dev --port 1947",
    "build": "plasmo build",
    "build:plasmo": "plasmo build",
    "build:next": "next build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@hookform/resolvers": "^3.2.0",
    "@mozilla/readability": "^0.4.4",
    "@plasmohq/messaging": "^0.5.0",
    "@plasmohq/storage": "^1.7.2",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/supabase-js": "^2.32.0",
    "@xenova/transformers": "^2.6.1",
    "add": "^2.0.6",
    "ai": "^2.2.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "driver.js": "^1.3.0",
    "input": "^1.0.1",
    "lru-cache": "^10.0.1",
    "lucide-react": "^0.265.0",
    "mark.js": "^8.11.1",
    "markdown-to-jsx": "^7.3.2",
    "next": "^13.5.3",
    "npm-run-all": "^4.1.5",
    "openai": "^4.15.3",
    "plasmo": "0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-markdown": "^9.0.0",
    "shadcn-ui": "^0.4.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.21.4"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/chrome": "0.0.245",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@xenova/transformers": "^2.6.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage",
      "activeTab",
      "scripting",
      "offscreen"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    },
    "cross_origin_embedder_policy": {
      "value": "require-corp"
    },
    "cross_origin_opener_policy": {
      "value": "same-origin"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "src/style.css",
          "iframe.html",
          "src/offscreen/offscreen.html",
          "src/offscreen/offscreen.js",
          "src/ML/**/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      },
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ],
    "commands": {
      "dosearch": {
        "suggested_key": {
          "default": "Alt+G",
          "windows": "Alt+G",
          "mac": "Alt+G",
          "linux": "Alt+G"
        },
        "description": "SHORTCUT: Show search | Modify it ðŸ‘‰"
      }
    }
  }
}
